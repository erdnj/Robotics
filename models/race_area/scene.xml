<mujoco model="race_area">
  <compiler angle="radian" />
  <include file="model.xml"/>
  <default>
    <default class="Way_line">
        <geom type="box" size="0.1 2.0 0.2" rgba="0 1 0 1"  solref="-10000 -10"/>
      </default>
  </default>
  <visual>
    <headlight diffuse="0.5 0.5 0.5"  ambient="0.6 0.6 0.6" specular="0.2 0.2 0.2"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <global azimuth="120" elevation="-20"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512"
        height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4"
        rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5"
        reflectance="0.2"/>
  </asset>

  <worldbody>
    <light pos="0 0 1.5" dir="0 0 -1" directional="true"/>
    <geom contype="1" friction=".5 0.005 0.0001" name="floor" pos="0 0 0" size="0 0 .25" type="plane" material="groundplane" condim="3"/>
    <body pos="2 0 0" euler="0 0 -1.57079633">
      <body euler="0 0 0" pos="0 0 0" >
        <geom pos="-0.7 0 0" class="Way_line"/>
        <geom pos="0.7 0 0" class="Way_line"/>
      </body>

      <body euler="0 0 0" pos="0 4.0 0">
        <geom pos="-0.7 0 0" class="Way_line"/>
        <geom pos="0.7 0 0" class="Way_line"/>
      </body>

      <body euler="0 0 1.57079633" pos="0 6.7 0">
        <geom pos="-0.7 -2.7 0" class="Way_line"/>
        <geom pos="0.7 -1.3 0" class="Way_line"/>
        <geom pos="-1.3 0.7 0" euler="0 0 1.57079633" class="Way_line"/>
      </body>

      <body euler="0 0 -1.57079633" pos="8.0 6.7 0">
        <geom pos="-0.7 -2.7 0" class="Way_line"/>
        <geom pos="0.7 -1.3 0" class="Way_line"/>
        <geom pos="-1.3 0.7 0" euler="0 0 1.57079633" class="Way_line"/>
      </body>

      <body euler="0 0 0" pos="8.0 9.3 0">
        <geom pos="-0.7 0 0" class="Way_line"/>
        <geom pos="0.7 0 0" class="Way_line"/>
      </body>

      <body euler="0 0 0" pos="8.0 13.3 0">
        <geom pos="-0.7 0 0" class="Way_line"/>
        <geom pos="0.7 0 0" class="Way_line"/>
      </body>
    </body>
<!--    <body euler="0 0 0" pos="17.3 -8 0">-->
<!--        <geom type="box" size="0.1 0.1 0.1" rgba="1 0 0 1"/>-->
<!--    </body>-->
  </worldbody>
</mujoco>
